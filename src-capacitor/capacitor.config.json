{
  "appId": "com.xavoc.shoutout",
  "appName": "SOS Bharat",
  "webDir": "www",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "Geolocation": {
      "permissions": [
        "location"
      ]
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "Filesystem": {
      "directory": "Cache"
    }
  },
  "android": {
    "allowMixedContent": true,
    "permissions": [
      "android.permission.READ_EXTERNAL_STORAGE",
      "android.permission.WRITE_EXTERNAL_STORAGE"
    ]
  }
}